create or replace table `projek-kimia-farma-486503.kimia_farma.agregasi` as
select
ft.transaction_id,
ft.date,
ft.branch_id,
kc.branch_name,
kc.kota,
kc.provinsi,
kc.rating as rating_cabang,
ft.customer_name,
ft.product_id,
inv.product_name,
ft.price as actual_price,
ft.discount_percentage,
case 
when ft.price<=50000 then 0.10
when ft.price>50000 then 0.15
when ft.price>100000 then 0.20
when ft.price>300000 then 0.25
else 0.30
end as persentase_gross_laba,
ft.price*(1-ft.discount_percentage) as nett_sales,
round(ft.price*(1-ft.discount_percentage) * case 
when ft.price<=50000 then 0.10
when ft.price>50000 then 0.15
when ft.price>100000 then 0.20
when ft.price>300000 then 0.25
else 0.30 
end,1) as nett_profit,
ft.rating as rating_transaksi
from 
`projek-kimia-farma-486503.kimia_farma.kf_final_transaction` as ft
left join 
`projek-kimia-farma-486503.kimia_farma.kf_kantor_cabang` as kc
on ft.branch_id=kc.branch_id
left join
`projek-kimia-farma-486503.kimia_farma.kf_inventory` as inv
on ft.branch_id=inv.branch_id
and ft.product_id=inv.product_id 
left join
`projek-kimia-farma-486503.kimia_farma.kf_product` as prd
on ft.product_id=prd.product_id
and ft.price=prd.price
